            var BreathTimes= $('#breathNumber').val();
            var BreathResults = GetBreathTest();
            var Condition = GetCondition();

            var LatPost = "0";
            var LongPost = "0";

            modal();

            setTimeout(function () {

                geocoder = new google.maps.Geocoder();
                var addressb = Postal + "," + Country;
                geocoder = new google.maps.Geocoder();
                geocoder.geocode({
                    'address': addressb
                }, function (results, status) {

                    var countryRes = "";
                    var administrative_area_level_5Res = "";
                    var administrative_area_level_4Res = "";
                    var administrative_area_level_3Res = "";
                    var administrative_area_level_2Res = "";
                    var administrative_area_level_1Res = "";

                    if (status == google.maps.GeocoderStatus.OK) {

                        LatPost = results[0].geometry.location.lat();
                        LongPost = results[0].geometry.location.lng();

                        geocoder.geocode({ 'latLng': results[0].geometry.location }, function (results, status) {
                            if (status == google.maps.GeocoderStatus.OK) {
                                    for (i = 0; i < results.length; i++) {
                            for (j = 0; j < results[i].address_components.length; j++) {

                                if (results[i].address_components[j].types[0] == 'country') {
                                    countryRes = results[i].address_components[j].long_name;
                                }
                                else if (results[i].address_components[j].types[0] == 'administrative_area_level_5') {
                                    administrative_area_level_5Res = results[i].address_components[j].long_name;
                                }
                                else if (results[i].address_components[j].types[0] == 'administrative_area_level_4') {
                                    administrative_area_level_4Res = results[i].address_components[j].long_name;
                                }
                                else if (results[i].address_components[j].types[0] == 'administrative_area_level_3') {
                                    administrative_area_level_3Res = results[i].address_components[j].long_name;
                                }
                                else if (results[i].address_components[j].types[0] == 'administrative_area_level_2') {
                                    administrative_area_level_2Res = results[i].address_components[j].long_name;
                                }
                                else if (results[i].address_components[j].types[0] == 'administrative_area_level_1') {
                                    administrative_area_level_1Res = results[i].address_components[j].long_name;
                                }

                            }
                                }

                                if (Country == countryRes)
                                {
                                                $.ajax({
                                        type: 'GET',
                                        url: '@Url.Action("SaveAssesment", "Home")',
                                        data: {
                                            UserID: userId,
                                            Mobile: Mobile,
                                            Evaluation: Result,
                                            LatBrowser: LatBrowser,
                                            LongBrowser: LongBrowser,
                                            LatPost: LatPost,
                                            LongPost: LongPost,
                                            Postal: Postal,
                                            Country: Country,
                                            PreDiseases: PreDiseases,
                                            Age: Age,
                                            Fever: Fever,
                                            Cough: Cough,
                                            DryCough: DryCough,
                                            Breath: Breath,
                                            BreathTimes: BreathTimes,
                                            BreathResults: BreathResults,
                                            Condition: Condition,
                                            Administrative_area_level_5: administrative_area_level_5Res,
                                            Administrative_area_level_4: administrative_area_level_4Res,
                                            Administrative_area_level_3: administrative_area_level_3Res,
                                            Administrative_area_level_2: administrative_area_level_2Res,
                                            Administrative_area_level_1: administrative_area_level_1Res
                                        },
                                        dataType: "json",
                                        cache: false,
                                        async: false,
                                        success: function (data) {

                                            var usuarios = getCookie("Users");
                                            if (usuarios == "")
                                                usuarios = "[]";
                                            var obj = JSON.parse(usuarios);
                                            var existUser = false;
                                            for (var i = 0; i < obj.length; i++)
                                            {
                                                if (obj[i].ID == data)
                                                    existUser = true;
                                            }
                                            if (existUser==false)
                                                obj.push({ "name": nickname, "ID": data });

                                            var myJSON = JSON.stringify(obj);
                                            setCookie("Users", myJSON, 10);
                                            setCookie("UserID", data, 10);
                                            $('#myModal').modal("hide");
                                        },
                                        error: function (errorMsg) {
                                           // alert("Data no de guardo");
                                        }
                                                });
                                }
                                else
                                {
                                    $('#myModal').modal("hide");
                                }



                            } else {
                                alert("Geocoder failed due to: " + status);
                            }
                        });
                    }
                    $('#myModal').modal("hide");

                });


            }, 1000);


            var f = 22;
        }
