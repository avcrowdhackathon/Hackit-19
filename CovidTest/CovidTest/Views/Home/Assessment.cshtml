        function myFunctionBreath() {

            if (seconds < 41 && seconds > 35) {
                document.getElementById("orderTimer").innerHTML = "@Localizer["Activity_prep1"]";
                $("#buttBreathTestStop").hide();
                $("#restartBreath").hide();
            }
            else if (seconds < 36 && seconds > 32) {
                document.getElementById("orderTimer").innerHTML = "@Localizer["Activity_prep2"]";
                document.getElementById("timer").innerHTML = (seconds - 30);
                $("#timer").css("color", "#F7AA18");
                $("#buttBreathTestStop").hide();
                $("#restartBreath").hide();
            }
            else if (seconds < 33 && seconds > 30) {
                document.getElementById("orderTimer").innerHTML = "@Localizer["Activity_prep2"]";
                document.getElementById("timer").innerHTML = (seconds - 30);
                $("#timer").css("color", "#D81515");
                $("#buttBreathTestStop").hide();
                $("#restartBreath").hide();
            }
            else {
                document.getElementById("orderTimer").innerHTML = "@Localizer["Activity_Screen_line1"] ";
                document.getElementById("orderTimerb").innerHTML = "@Localizer["Activity_Screen_line2"] ";
                $("#buttBreathTestStop").show();
                $("#restartBreath").show();
            }
            if (seconds < 31) {

                $("#timer").css("color", "#15C5D8");
                document.getElementById("timer").innerHTML = seconds;
            }
            if (seconds > 0) {
                seconds--;
            } else {
                clearInterval(timer);
                current_fs = $("#BreathTest2");
                next_fs = $("#BreathTest3");
                $("#progressBarAssessment").width(80 + "%");
                $("#Steps").text('@Localizer["Screen8_Step"]');
                moveForward(current_fs, next_fs);
            }
        }
