        function SaveResults(Result) {

            var userId = "";

            var Mobile = window.mobilecheck();

            if (checkCookie("UserID")) {
                userId = getCookie("UserID")
            }
            var LatBrowser = "0";
            if (checkCookie("latitude")) {
